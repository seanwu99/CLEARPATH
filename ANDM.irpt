        function roleSecurity() {

            selectedUnitName = document.getElementById("unit_name").value;				//   SETING
            selectedMapStatusID = document.getElementById("status_id").value;				//   SETING

            selectedTimeFrom = '01-01-2017 00:00';								//   SETING
