<mvc:View controllerName="com.test.northwind.view.Detail" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Page showNavButton="{device>/isPhone}" navButtonPress="onNavBack" class="sapUiFioriObjectPage" title="{i18n>detailTitle}">
		<content>
			<ObjectHeader title="{ProductName}" number="{UnitPrice}" numberUnit="{QuantityPerUnit}">
				<ObjectAttribute text="{UnitsInStock}"/>
				<ObjectAttribute text="{UnitsOnOrder}"/>
			</ObjectHeader>
			<IconTabBar select="onDetailSelect" id="idIconTabBar">
				<items>
					<IconTabFilter key="Supplier" text="Supplier" icon="sap-icon://hint">
						<!--key="Detail" -->
						<content>
							<core:Fragment fragmentName="com.test.northwind.view.DetailInfoForm" type="XML"/>
						</content>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>