        function findParent(node) {
            var parent;
            var allNodes = [];
            getBranch(floorTree.getNodes()[0], allNodes);
            // 		ss(allNodes.length);
            $.each(allNodes, function (i, _node) {
                // 			ss(_node.getText());
                if (_node.getNodes().length > 0) {
                    $.each(_node.getNodes(), function (i, mm) {
                        if (mm.getText() == node.getText()) {
                            // 						ss(_node.getText());
                            parent = _node;
                            return false;
                        }
                    });
                }
            });
            return parent;
        }
