        function rmData(n, m) {
            var aData = [];
            if (n == 1) {
                for (var i = 0; i < m; i++) {
                    aData.push(60 + Math.round((Math.random() * 40)));
                }
            } else if (n == 2) {
                var ts = Math.round((new Date().getTime() - n * 1000) / 1000) * 1000;
                for (var i = 0; i < m; i++) {
                    aData.push([ts + i * 60 * 1000, 45 + Math.round((Math.random() * 55))]);
                }
            }
            return aData;
        }

