        function chartOeeTrendLoad(_title) {
            var a, p, q;
            var OEE = [];
            var Availability = [];
            var Performance = [];
            var Quality = [];
            var n = 1;
            var tm = new Date().getTime() - n * 1000;
            for (var i = 0; i < n; i++) {
                tm += i * 1000;
                a = 85 + Math.round((Math.random() * 14));
                p = 85 + Math.round((Math.random() * 14));
                q = 85 + Math.round((Math.random() * 14));
                Availability.push([tm, a]);
                Performance.push([tm, p]);
                Quality.push([tm, q]);
                OEE.push([tm, a * p * q / 10000]);
            }
            var chartSeries = [];
            chartSeries.push({
                name: 'OEE',
                data: OEE
            });
            chartSeries.push({
                name: 'Availability',
                data: Availability
            });
            chartSeries.push({
                name: 'Performance',
                data: Performance
            });
            chartSeries.push({
                name: 'Quality',
                data: Quality
            });
            chartReady = false;
            chartOeeTrendInit(chartSeries, _title);
        }


